var Gameover = cc.Sprite.extend({
    ctor: function(GameLayer){
        this._super(); 
        this.initWithFile( 'res/images/pig.png' );
        this.gameLayer = GameLayer ;
        this.shoot = false ;
        
       
    }
});
var GameOverScreen = cc.Scene.extend({
        onEnter: function() {
            this._super();
            var layer = new GameOverPage();
            layer.init();
            this.addChild( layer );
        }
        
});